  "author": "foxundermoon",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "got": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@typescript-eslint/parser": "2.34.0",
    "@zeit/ncc": "0.20.5",
    "eslint": "5.16.0",
    "eslint-plugin-github": "2.0.0",
    "eslint-plugin-jest": "22.21.0",
    "jest": "24.9.0",
    "jest-circus": "24.9.0",
    "js-yaml": "3.13.1",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "typescript": "3.7.2"
  }
